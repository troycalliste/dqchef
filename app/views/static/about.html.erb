<% @ratings.each do |r| %>
<div class="reviewwrap">
    <div class="rrleft">
      <h2><%= r.name %></h2>
      <h3><%= r.location%></h3>
    </div>
    <div class="rrother">
      <div class="upperother">
        <% if r.rated == 1 %>
      <%= render "onestar" %>
      <% elsif r.rated == 2 %>
      <%= render "twostar" %>
      <% elsif r.rated == 3 %>
      <%= render "threestar" %>
      <% elsif r.rated == 4 %>
      <p>lok</p>
      <%= render "fourstar" %>
      <% elsif r.rated == 5 %>
      <%= render "fivestar" %>
      <% end %>
      <span><%= r.created_at.strftime("%m/%d/%Y") %></span>
      </div>

     <div class="review">
       <%= r.message %>
     </div>
    </div>
</div>

<% end %>



